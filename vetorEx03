algoritmo_vetorEx03{
    preco:vetor[5]de real;
    inteiro indice;
    real soma = 0, acrescimo;

    para(indice = 1 ate 5 passo1)faca
        escreva("Informe valor produto [",indice,"] :");
        leia(preco[indice]);
        soma = soma + preco[indice];
    fimpara

    acrescimo = preco[3]*1,1;

    escreva("A soma total Ã© : ",soma);
    escreva("Produto [3] + 10% = ",acrescimo);

}fimalgoritmo